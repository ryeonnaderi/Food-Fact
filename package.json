    "mysql2": "^1.6.1",
    "path": "^0.12.7",
    "sequelize": "^4.38.0"
  },
  "devDependencies": {
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1",
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3"
  }
}
